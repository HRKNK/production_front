{
	"name": "full_fronted_react",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"server:json": "node ./json-server/index.js",
		"start": "webpack serve --env port=3000",
		"start:dev": "concurrently \"npm run start\" \"npm run server:json\"",
		"start:vite": "vite --open",
		"start:dev:vite": "concurrently \"npm run start:vite\" \"npm run server:json\"",
		"build:prod": "webpack --env mode=production",
		"build:dev": "webpack --env mode=development",
		"eslint": "npx eslint **/*.{ts,tsx}",
		"eslint:fix": "npm run eslint -- --fix",
		"prettier:fix": "npx prettier --write **/*.{ts,tsx,json}",
		"stlint": "npx stylelint **/*.scss",
		"stlint:fix": "npm run stlint -- --fix",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:loki": "npx loki test",
		"test:loki:ok": "npx loki approve",
		"test:loki:report": "node scripts/generate-visual-json-report.js && npx reg-cli --from .loki/report.json --report .loki/report.html",
		"test:loki:ci": "npx loki --requireReference --reactUri file:./storybook-static",
		"storybook": "start-storybook -p 6006 -c ./config/storybook",
		"storybook:build": "build-storybook -c ./config/storybook",
		"generate:slice": "node ./scripts/createSlice/index.js",
		"postinstall": "rmdir /s /q .\\node_modules\\.cache"
	},
